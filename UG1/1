#include<stdio.h>
#include<math.h>
void main(){
	float a,b,c,D,r1,r2;
		printf("enter the coefficient of x^2:\n");
		printf("enter the coefficient of x:\n");
		printf("enter the constant term\n");
	scanf("%f%f%f",&a&b&c);
					D=(b*b)-(4*a*c);
if(D<0)
	printf("oops!  can't find complex roots");
else
			p= Pow(D,0.5);
			r1= (b+p)/2*a;
			r2= (b-p)/2*a;
	printf("the roots are:%f%f",r1,r2);
	result 0;
}
